<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE tiles-definitions PUBLIC
       "-//Apache Software Foundation//DTD Tiles Configuration 3.0//EN"
       "http://tiles.apache.org/dtds/tiles-config_3_0.dtd">
 
<tiles-definitions>

    <definition name="agency" template="/WEB-INF/jsp/agency/layout/agencyLayout.jsp">
        <put-attribute name="header" value="/WEB-INF/jsp/agency/layout/agencyHeader.jsp"/>
        <put-attribute name="footer" value="/WEB-INF/jsp/agency/layout/agencyLeft.jsp"/>
        <put-attribute name="body" value=""/>
    </definition>
    
    <definition name="agency/login/agencyLogin" template="/WEB-INF/jsp/agency/login/agencyLogin.jsp">
    </definition>
    
    <definition name="agency/main/agencyMain" extends="agency">
        <put-attribute name="body" value="/WEB-INF/jsp/agency/main/agencyMain.jsp"/>
    </definition>
    
    <definition name="agency/*/*" extends="agency">
        <put-attribute name="body" value="/WEB-INF/jsp/agency/{2}/{3}.jsp"/>
    </definition>
    
    
</tiles-definitions>


